<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="NormalEditText">
        <attr name="only_click" format="boolean"></attr>
        <attr name="must" format="boolean"></attr>
        <attr name="text_title" format="string"></attr>
        <attr name="text_hint" format="string"></attr>
        <attr name="text_type" format="dimension"></attr>
        <attr name="text_alignment" format="string"></attr>
        <attr name="width_title" format="dimension"></attr>
        <attr name="linebackground" format="color"></attr>
    </declare-styleable>


    <declare-styleable name="ScrollingBackgroundView">
        <attr name="sbv_drawable" format="reference"/>
        <attr name="sbv_scrollX" format="dimension"/>
        <attr name="sbv_scrollY" format="dimension"/>
    </declare-styleable>

</resources>
